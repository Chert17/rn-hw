{
  "jsxSingleQuote": true,
  "singleQuote": true,
  "semi": true,
  "tabWidth": 2,
  "trailingComma": "all",
  "printWidth": 80,
  "bracketSameLine": false,
  "useTabs": false,
  "arrowParens": "always",
  "endOfLine": "auto",
  "importOrder": ["<THIRD_PARTY_MODULES>", "@/(.*)", "^[./ ]"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
